page {
  background: #FFFFFF;
}
.zaiui-goods-details-box {
  position: relative;
  padding: 27.27rpx;
}
.zaiui-goods-details-box .cu-avatar {
  position: absolute;
  width: 81.81rpx;
  height: 81.81rpx;
}
.zaiui-goods-details-box .goods-info-view {
  position: relative;
  padding-left: 109.09rpx;
  height: 81.81rpx;
  line-height: 1.7;
}
.zaiui-goods-details-box .zaiui-goods-tip-view {
  position: relative;
  margin-top: 27.27rpx;
}
.zaiui-hight-view {
  width: 100%;
  height: 36.36rpx;
  background: #FAFAFA;
}
.zaiui-appraise-view {
  position: relative;
  background-color: #F8F8F8;
  margin: 27.27rpx 27.27rpx 27.27rpx 136.36rpx;
  border-radius: 9.09rpx;
  padding: 21.81rpx;
}
.zaiui-appraise-view .title-view {
  position: relative;
}
.zaiui-appraise-view .title-view .name {
  position: absolute;
}
.zaiui-appraise-view .title-view .content {
  position: relative;
  padding-left: 136.36rpx;
}
.zaiui-appraise-view .time-view {
  position: relative;
  margin-top: 14.54rpx;
}
.zaiui-appraise-view .time-view .time {
  position: relative;
  padding-right: 145.45rpx;
}
.zaiui-appraise-view .time-view .status {
  position: absolute;
  right: 0;
  top: 0;
}
.zaiui-foot-tip-view {
  position: relative;
  text-align: center;
  margin-top: 72.72rpx;
  margin: 27.27rpx 0;
}
.zaiui-foot-tip-view .img-view {
  position: relative;
  margin-bottom: 9.09rpx;
}
.zaiui-foot-tip-view .img-view .cu-avatar {
  background: #FFFFFF;
  background-size: cover;
  background-position: center;
}
.zaiui-btn-view {
  position: fixed;
  width: 100%;
  bottom: calc(env(safe-area-inset-bottom) / 2);
}
.zaiui-btn-view .flex {
  padding: 18.18rpx;
}

